        <img src="{{ file_url(content.field_picture_2_queen['#items'].entity.uri.value) }}" alt="img">
        <div class="caption"> 
          <strong class="title">{{ node.field_main_title_2_queen.value }} <b>{{ node.field_sub_title_2_queen.value }}</b></strong> 
