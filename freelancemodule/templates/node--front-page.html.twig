              <h2><a href="{{ p.field_sub_title_2_b_h.0.url }}">{% if p.field_sub_title_2_b_h.0.title  is not empty %}{{ p.field_sub_title_2_b_h.0.title }}{% endif %}</a></h2>
              <a href="{{ p.field_catalogue_2_b_h.0.url }}" class="link">Parcourir le Catalogue<i class="fa fa-chevron-right" aria-hidden="true"></i></a> </div>
